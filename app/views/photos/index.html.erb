<h2>Insta, Your Photo Gallery</h2>
<div class="row">
<% @photos.each do |photo| %>
<div class="col-md-3 col-sm-4 col-xs-6">
<%= link_to (image_tag photo.photo_file_name.url(:medium)), photo_path(photo) %><br>
<b><%= photo.name %></b><br>
<%= link_to "Edit", edit_photo_path(photo) %> |
<%= link_to "Delete", photo_path(photo), :method => :delete, :data => {confirm: "Are you sure want to delete this photo?"} %>
</div>
<% end %>
</div>
